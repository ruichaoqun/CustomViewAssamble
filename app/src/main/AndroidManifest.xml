<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.customviewassamble">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.HobbyViewActivity" />
        <activity android:name=".ui.WaveActivity" />
        <activity android:name=".ui.AnalogControllerActivity" />
        <activity android:name=".ui.LineChatViewActivity" />
        <activity android:name=".ui.SortActivity" />
        <activity
            android:name=".ui.BubbleSortActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ui.SelectionSortActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ui.InsertSortActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ui.QuickSortActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ui.ShellSortActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".ui.DrawCardActivity"></activity>
    </application>

</manifest>